
<h1>Registration</h1>

<%= form_for :user, url: users_path do |f| %>
	<p><%= f.label :firstname %>
	<%= f.text_field :firstname %></p>
 
 	<p><%= f.label :lastname %>
	<%= f.text_field :lastname %></p>

	<p><%= f.label :email %>
	<%= f.text_field :email %></p>

	<p><%= f.label :password %>
	<%= f.password_field :password %></p>


	<p><%= f.submit "Save" %></p>

<%end%>